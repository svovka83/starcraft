generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id Int @id @default(autoincrement())

  fullName String
  email    String
  password String

  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model One {
  id Int @id @default(autoincrement())

  mana Int

  minerals Int
  mine     Int
  boss     Int

  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model Two {
  id Int @id @default(autoincrement())

  mana Int

  minerals Int
  mine     Int
  boss     Int

  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}
